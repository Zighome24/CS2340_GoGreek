<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        android:text="Key:"
        android:textSize="14sp"
        android:id="@+id/textView5" />

    <TextView
        android:id="@+id/txt_RSKey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="15dp"
        android:autoText="false"
        android:editable="true"
        android:text="none" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_RSKey"
        android:layout_below="@+id/txt_RSKey"
        android:layout_marginTop="30dp"
        android:text="Date:" />

    <TextView
        android:id="@+id/txt_RSDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView8"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="20dp"
        android:text="none" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_RSDate"
        android:layout_below="@+id/txt_RSDate"
        android:layout_marginTop="25dp"
        android:text="Location Type:" />

    <TextView
        android:id="@+id/txt_RSLocationType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="20dp"
        android:text="none" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView19"
        android:layout_marginStart="150dp"
        android:layout_toEndOf="@+id/textView19"
        android:text="Zip:" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/txt_RSLocationType"
        android:layout_marginTop="25dp"
        android:layout_toEndOf="@+id/txt_RSLocationType"
        android:text="Address:" />

    <TextView
        android:id="@+id/txt_RSAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView17"
        android:layout_below="@+id/textView17"
        android:layout_marginTop="20dp"
        android:layout_toEndOf="@+id/txt_RSLocationType"
        android:text="none" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView17"
        android:layout_below="@+id/txt_RSAddress"
        android:layout_marginTop="25dp"
        android:layout_toEndOf="@+id/txt_RSAddress"
        android:text="City:" />

    <TextView
        android:id="@+id/txt_RSCity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView19"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="20dp"
        android:layout_toEndOf="@+id/txt_RSAddress"
        android:text="none" />

    <TextView
        android:id="@+id/txt_RSZip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView14"
        android:layout_alignTop="@+id/txt_RSCity"
        android:text="None" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_RSCity"
        android:layout_below="@+id/txt_RSCity"
        android:layout_marginTop="25dp"
        android:text="Borough:" />

    <TextView
        android:id="@+id/txt_RSBorough"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView22"
        android:layout_below="@+id/textView22"
        android:layout_marginTop="16dp"
        android:text="none" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_RSBorough"
        android:layout_below="@+id/txt_RSBorough"
        android:layout_marginTop="25dp"
        android:text="Lat:" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txt_RSZip"
        android:layout_alignTop="@+id/textView24"
        android:text="Long:" />

    <TextView
        android:id="@+id/txt_RSLat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView24"
        android:layout_below="@+id/textView24"
        android:layout_marginTop="10dp"
        android:text="none" />

    <TextView
        android:id="@+id/txt_RSLong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView25"
        android:layout_alignTop="@+id/txt_RSLat"
        android:text="none" />
</RelativeLayout>